{
    "name": "orclapex-autocomplete",
    "displayName": "Intellisense for the Oracle Application Express Developer",
    "description": "Intellisense support for Oracle Database and Oracle Application Express APIs",
    "version": "0.0.1",
    "publisher": "apng",
    "author": {
      "name": "Adrian Png",
      "email": "adrian.png@fuzziebrain.com",
      "url": "https://github.com/fuzziebrain"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/fuzziebrain/orclapex-autocomplete"
    },
    "icon": "images/apex.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
      "plsql",
      "apex",
      "orclapex",
      "oracle",
      "intellisense"
    ],
    "activationEvents": [
        "onLanguage:plsql"
    ],
    "main": "./out/extension",
    "contributes": {
    },
    "extensionDependencies":[
      "xyz.plsql-language"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
